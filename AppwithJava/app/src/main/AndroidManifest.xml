<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anna.appwithjava">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:configChanges = "keyboardHidden|orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <activity android:label="@string/app_name" android:name="NextActivity"/>
            </intent-filter>
        </activity>
        <activity android:name=".Zamowienie"
            android:label="@string/title_activity_zamowienie"
            android:parentActivityName="com.example.anna.appwithjava.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.anna.appwithjava.Zamowienie" />

        </activity>
    </application>

</manifest>